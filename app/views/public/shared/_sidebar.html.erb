<div class="col-md-2">
  <table>
    <thead>
      <tr>
        <th>ジャンル検索</th>
      </tr>
    </thead>
    <tbody>
      <%= genres.each do |genre| %>
      <tr>
        <td><%= link_to genre.name,search_items_path(genre.id) method: :get %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>